      RXB0D1 <= 6;         -- NN high
      RXB0D2 <= 5;         -- NN low
      RXB0D3 <= 4;
      RXB0D4 <= 2;
      RXB0CON.RXFUL <= '1';
      RXB0DLC.DLC3 <= '1';
      CANSTAT <= 16#0C#;
      PIR3.RXB0IF <= '1';
      --
      wait on PORTC for 1005 ms;
      if PORTC != 0 then
        report("slim_boot_unlearn_test: Unexpected trigger");
        test_state := fail;
        wait until PORTC == 0;
      end if;
      --
      if test_state == pass then
        report("slim_boot_unlearn_test: PASS");
      else
        report("slim_boot_unlearn_test: FAIL");
      end if;          
      PC <= 0;
      wait;
    end process slim_boot_unlearn_test;
end testbench;
